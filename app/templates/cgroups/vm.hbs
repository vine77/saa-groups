<div class="box box-solid box-success component collapsed-box">
  <div class="box-header">
    <h3 class="box-title">
      <i class="fa fa-cube"></i>
      {{name}}
    </h3>
    <div class="box-tools pull-right">
      <button class="btn btn-success btn-sm" type="button">
        Details
        <i class="fa fa-toggle-right"></i>
      </button>
    </div>
  </div>
  <div class="box-subheader clearfix">
    <div class="component-status">
      <div class="component-status-state">
        <i class="fa fa-fw fa-check-circle success"></i>
        <i class="fa fa-fw fa-bolt on"></i>
      </div>
      <div class="component-status-other">
        <i class="fa fa-fw fa-trophy assured"></i>
        <i class="fa fa-fw fa-lock unregistered"></i>
        <i class="fa fa-fw fa-bullhorn error" style="color:#9d261d"></i>
      </div>
    </div><div class="component-capacity">
      <div>2 vCPUs &times; 0.5-1.0 SCU = 1.0-2.0 SCUs</div>
      <div class="clearfix">
        <small class="pull-left">Memory (GiB)</small>
        <small class="pull-right">31%</small>
      </div>
      <div class="progress xs">
        <div class="progress-bar progress-bar-green" {{bind-attr style="memoryStyle"}}></div>
      </div>
      <div class="clearfix">
        <small class="pull-left">15</small><small class="pull-right">48</small>
      </div>
    </div><div class="component-charts">
      {{!-- <img src="/assets/images/dials.png"> --}}
    </div>
  </div>
</div>
